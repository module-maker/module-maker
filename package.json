    "routes-router": "~1.1.3",
    "serve-browserify": "~0.3.3",
    "st": "~0.2.2",
    "send-data": "~3.1.2",
    "bops": "~0.1.0",
    "js-git": "~0.6.0",
    "uuid": "~1.4.1",
    "mkdirp": "~0.3.5",
    "latest": "~0.1.2",
    "body": "~2.0.1",
    "global": "~2.0.7",
    "domlog": "0.0.9",
    "xhr": "~1.2.3",
    "string-template": "~0.1.1",
    "after": "~0.8.1",
    "xtend": "~2.1.1",
    "by": "~0.2.3",
    "request": "~2.27.0",
    "detective": "~2.2.0",
    "browser-module-sandbox": "Matt-Esch/browser-module-sandbox",
